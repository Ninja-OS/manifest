<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="github"
           fetch="https://github.com" />

  <remote  name="ninja"
           fetch="https://github.com/Ninja-OS" />

  <default revision="o8.x"
           remote="ninja"
           sync-f="true"
           sync-c="true"
           sync-j="8" />

  <!-- NinjaOS -->
  <project path="manifest" name="manifest" remote="ninja" />
  <project path="build/make" name="build" remote="ninja" >
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>
  <project path="build/kati" name="build_kati" groups="pdk,tradefed" remote="ninja" />
  <project path="build/soong" name="build_soong" groups="pdk,tradefed" remote="ninja" >
    <linkfile src="root.bp" dest="Android.bp" />
    <linkfile src="bootstrap.bash" dest="bootstrap.bash" />
  </project>

  <project path="bionic" name="bionic" groups="pdk" remote="ninja" />
  <project path="bootable/recovery" name="bootable_recovery" groups="pdk" remote="ninja" />
  <project path="development" name="development" groups="pdk-cw-fs,pdk-fs" remote="ninja" />
  <project path="device/ninja/sepolicy" name="device_ninja_sepolicy" remote="ninja" />
  <project path="external/connectivity" name="external_connectivity" groups="pdk" remote="ninja" />
  <project path="external/ebtables" name="external_ebtables" groups="pdk" remote="ninja" />
  <project path="external/exfat" name="external_exfat" remote="ninja" />
  <project path="external/fuse" name="external_fuse" remote="ninja" />
  <project path="external/htop" name="external_htop" remote="ninja" />
  <project path="external/json-c" name="external_json-c" remote="ninja" />
  <project path="external/libncurses" name="external_libncurses" remote="ninja" />
  <project path="external/libnfc-nxp" name="external_libnfc-nxp" remote="ninja" />
  <project path="external/mksh" name="external_mksh" groups="pdk" remote="ninja" />
  <project path="external/nano" name="external_nano" remote="ninja" />
  <project path="external/skia" name="external_skia" groups="pdk,qcom_msm8x26" remote="ninja" />
  <project path="external/tinycompress" name="external_tinycompress" groups="pdk" remote="ninja" />
